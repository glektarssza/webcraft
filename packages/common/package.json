{
    "name": "@glektarssza/webcraft-common",
    "description": "Common library for the Webcraft project.",
    "version": "0.0.0",
    "license": "SEE LICENSE IN LICENSE.md",
    "private": true,
    "author": {
        "name": "G'lek Tarssza",
        "email": "glek@glektarssza.com",
        "url": "https://glektarssza.com"
    },
    "files": [
        "dist",
        "LICENSE.md",
        "README.md",
        "package.json"
    ],
    "type": "module",
    "exports": {
        ".": {
            "browser": {
                "development": {
                    "import": "./dist/dev/webcraft-common.js",
                    "require": "./dist/dev/webcraft-common.umd.cjs"
                },
                "production": {
                    "import": "./dist/prod/webcraft-common.js",
                    "require": "./dist/prod/webcraft-common.umd.cjs"
                },
                "import": "./dist/prod/webcraft-common.js",
                "require": "./dist/prod/webcraft-common.umd.cjs",
                "types": "./dist/types/index.d.ts",
                "default": "./dist/prod/webcraft-common.umd.cjs"
            },
            "development": {
                "import": "./dist/dev/webcraft-common.js",
                "require": "./dist/dev/webcraft-common.umd.cjs"
            },
            "production": {
                "import": "./dist/prod/webcraft-common.js",
                "require": "./dist/prod/webcraft-common.umd.cjs"
            },
            "node": null,
            "types": "./dist/types/index.d.ts",
            "import": "./dist/prod/webcraft-common.js",
            "require": "./dist/prod/webcraft-common.umd.cjs",
            "default": "./dist/prod/webcraft-common.umd.cjs"
        }
    },
    "keywords": [
        "webcraft",
        "common",
        "library"
    ],
    "homepage": "https://github.com/glektarssza/webcraft",
    "bugs": {
        "url": "https://github.com/glektarssza/webcraft/issues"
    },
    "repository": {
        "type": "git",
        "directory": "packages/webcraft-common",
        "url": "https://github.com/glektarssza/webcraft.git"
    },
    "scripts": {
        "test": "pnpm run test:base",
        "test:ci": "pnpm run test:coverage",
        "test:coverage": "pnpm run test:base --coverage",
        "test:base": "wtr",
        "lint": "pnpm run lint:cache ./src/ ./tests/",
        "lint:ci": "pnpm run lint:base ./src/ ./tests/",
        "lint:fix": "pnpm run lint:base --fix ./src/ ./tests/",
        "lint:cache": "pnpm run lint:base --cache",
        "lint:base": "eslint",
        "build": "pnpm run build:types && pnpm run build:prod",
        "build:all": "pnpm run build:types && pnpm run build:dev && pnpm run build:prod",
        "build:types": "tsc --project ./src/ --declaration --emitDeclarationOnly --outDir ./dist/types/",
        "build:dev": "vite build --mode development",
        "build:prod": "vite build --mode production",
        "clean": "pnpm run clean:types && pnpm run clean:prod",
        "clean:all": "pnpm run clean:types && pnpm run clean:dev && pnpm run clean:prod",
        "clean:types": "rimraf ./dist/types/",
        "clean:dev": "rimraf ./dist/dev/",
        "clean:prod": "rimraf ./dist/prod/",
        "rebuild": "pnpm run clean && pnpm run build",
        "rebuild:all": "pnpm run clean:all && pnpm run build:all",
        "rebuild:types": "pnpm run clean:types && pnpm run build:types",
        "rebuild:dev": "pnpm run clean:dev && pnpm run build:dev",
        "rebuild:prod": "pnpm run clean:prod && pnpm run build:prod"
    },
    "devDependencies": {
        "@eslint/js": "^9.4.0",
        "@faker-js/faker": "^8.4.1",
        "@remcovaes/web-test-runner-vite-plugin": "^1.2.1",
        "@rollup/plugin-replace": "^5.0.7",
        "@types/chai": "^4.3.16",
        "@types/eslint__js": "^8.42.3",
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.14.2",
        "@types/sinon": "^17.0.3",
        "@web/test-runner": "^0.18.2",
        "@web/test-runner-puppeteer": "^0.16.0",
        "chai": "^5.1.1",
        "eslint": "^9.4.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-tsdoc": "^0.3.0",
        "mocha": "^10.4.0",
        "prettier": "^3.3.1",
        "prettier-eslint": "^16.3.0",
        "rimraf": "^5.0.7",
        "sass": "^1.77.4",
        "sinon": "^18.0.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5",
        "typescript-eslint": "8.0.0-alpha.26",
        "vite": "^5.2.12"
    }
}
